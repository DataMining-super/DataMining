<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--public List<Users> showUserList();-->
<mapper namespace="com.swpu.mapper.UserDao">
    <select id="showUserList" resultType="Users" >
        select * from tb_users
    </select>
    <insert id="registerUser" useGeneratedKeys="true" keyProperty="userId" keyColumn="user_id" >
        insert into
        tb_users(user_id,username,password,local_auth,identities)
        values
        (#{userId},#{username},#{password},#{localAuth},#{identities})
    </insert>
</mapper>